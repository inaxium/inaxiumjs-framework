  "user": {
    "name": "User Application",
    "dir": "{{app.dir}}/user",
    "ds": {
      "name": "user",
      "ptr": true
    },
    "list": {
      "entry": {
        "nav": {
          "target": "{{app.target.nav}}",
          "url": "{{pub.dir}}/nav/list"
        },
        "main": {
          "target": "{{app.target.main}}",
          "url": "{{user.dir}}/list"
        }
      },
      "scroll": {
        "x": 0,
        "y": 0
      }
    },
    "site": {
      "entry": {
        "nav": {
          "target": "{{app.target.nav}}",
          "url": "{{pub.dir}}/nav/site"
        },
        "main": {
          "target": "{{app.target.main}}",
          "url": "{{user.dir}}/site"
        }
      }
    },
    "rest": {
      "url": {
        "equal": true,
        "any": "/user"
      }
    },
    "modal": {
      "show": {
        "click": {
          "emit1": {
            "key": "modal.user",
            "constraint": "show"
          },
          "emit2": {
            "key": "sidebar",
            "constraint": "close",
            "data": {
              "nodes": {
                "sidebar": "public.sidebar.ptr"
              }
            }
          }
        }
      }
    }
